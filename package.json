  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karlapraveenkumar/Proshop-V2/issues"
  },
  "homepage": "https://github.com/karlapraveenkumar/Proshop-V2#readme",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "nodemon": "^3.1.10"
  }
}
